{
  "name": "ts_node_typeorm_postgres_docker",
  "version": "1.1.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "cross-env NODE_ENV=dev npx tsx watch src/server.ts",
    "prod": "tsc && node dist/server.js",
    "typeorm:cli": "ts-node ./node_modules/typeorm/cli",
    "migration:create": "cross-env NAME=$npm_config_name npm run typeorm:cli -- migration:create src/migrations/%npm_config_name%",
    "migration:run": "cross-env NODE_ENV=dev npm run typeorm:cli -- migration:run -d src/config/orm.config",
    "migration:revert": "cross-env NODE_ENV=dev npm run typeorm:cli -- migration:revert -d src/config/orm.config",
    "migration:generate": "cross-env NODE_ENV=dev NAME=$npm_config_name npm run typeorm:cli --migration:generate -n %npm_config_name%",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "lint:check": "eslint . --max-warnings=0",
    "lint:debug": "eslint . --debug",
    "test": "npx jest"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^5.1.1",
    "body-parser": "^1.20.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "envalid": "^8.0.0",
    "express": "^4.21.2",
    "jsonwebtoken": "^9.0.2",
    "pg": "^8.13.1",
    "reflect-metadata": "^0.2.2",
    "typeorm": "^0.3.20"
  },
  "devDependencies": {
    "@eslint/js": "^9.21.0",
    "@types/bcrypt": "^5.0.2",
    "@types/cookie-parser": "^1.4.8",
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.14",
    "@types/jsonwebtoken": "^9.0.8",
    "@types/node": "^22.10.7",
    "cross-env": "^7.0.3",
    "eslint": "^9.21.0",
    "eslint-config-prettier": "^10.0.2",
    "eslint-plugin-jest": "^28.11.0",
    "globals": "^16.0.0",
    "jest": "^29.7.0",
    "node-gyp": "^11.0.0",
    "nodemon": "^3.1.9",
    "prettier": "3.4.2",
    "ts-jest": "^29.2.6",
    "ts-node": "^10.9.2",
    "tslint-config-airbnb": "^5.11.2",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.25.0"
  }
}
